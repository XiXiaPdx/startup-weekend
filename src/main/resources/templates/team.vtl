<div class="container">
#if ($members.size()==0)
 <h1> There are no members</1>
#else
<h2> Team $team.getTeamName()</h2>
  #foreach ($member in $members)
   <div class="jumbotron teamJumbo">
    <h3 class="memberName"><a href ="/teams/$team.getTeamID()/member/$team.getTeamMembers().indexOf($member)">$member.getMemberName()</a> </h3>
   </div>
  #end
#end  
  <h4><a href="/teams">Add Another Member</a></h4>

   <h4><a href="/">Add More Teams</a></h4>
</div>
